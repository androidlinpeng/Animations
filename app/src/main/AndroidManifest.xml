<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.msgcopy.application">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />

    <application
        android:name=".activity.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.AnimationActivity" />
        <activity android:name=".activity.CustomCircleActivity" />
        <activity android:name=".activity.PaletteViewActivity" />
        <activity android:name=".activity.SemicircleViewActivity" />
        <activity android:name=".activity.GestureDetectorActivity" />
        <activity
            android:name=".activity.ProgressViewActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".activity.BlurryActivity" />
        <activity android:name=".activity.FrameAnimationActivity" />
        <activity android:name=".activity.LoadAnimActivity" />
        <activity android:name=".activity.PropertyAnimActivity" />
        <activity android:name=".activity.CircleLoadActivity" />
        <activity android:name=".activity.FicheActivity"></activity>
    </application>

</manifest>